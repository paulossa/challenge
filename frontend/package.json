{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "alt": "^0.18.6",
    "alt-utils": "^2.0.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "watch-less-compiler": "^1.4.0",
    "babel-eslint": "9.0.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "npm-run-all -lp watch-css start-js",
    "watch-css": "less-watch-compiler ./src/ ./src/",
    "start-js": "react-scripts start",
    "build": "npm-run-all build-css build-js",
    "build-css": "less-watch-compiler --run-once src/ ./src/",
    "build-js": "GENERATE_SOURCEMAP=false react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "pre-commit": "lint-staged",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  }
}
